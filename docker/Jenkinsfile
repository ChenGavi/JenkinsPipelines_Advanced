pipeline {
  agent {
    docker {
      image 'executeautomation/jenkinsagent:agent'
    }

  }
  stages {
    stage('Verify') {
      parallel {
        stage('Build') {
          steps {
              sh 'dotnet restore "$WORKSPACE/src/EAApp.csproj" --force'
               sh 'dotnet build "$WORKSPACE/src/EAApp.csproj"'
          }
        }

      }
    }
    


  }
  environment {
    SELENIUM = 'c:\\driver\\win'
    VARIABLE = ''
  }
}
